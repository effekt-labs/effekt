{
  "name": "effekt-workspace",
  "version": "0.0.0",
  "author": "Ivo Dolenc",
  "private": true,
  "type": "module",
  "scripts": {
    "format:root": "prettier --write . '!playgrounds' '!packages'",
    "format:effekt": "pnpm --filter effekt run format",
    "lint:effekt": "pnpm --filter effekt run lint",
    "test:effekt": "pnpm --filter effekt run test",
    "test:utils:effekt": "pnpm --filter effekt run test:utils",
    "test:easing:effekt": "pnpm --filter effekt run test:easing",
    "check:effekt": "pnpm --filter effekt run check",
    "update:effekt": "pnpm --filter effekt run update",
    "build:effekt": "pnpm --filter effekt run build",
    "prepublishOnly": "pnpm run build:effekt",
    "sync:sveltekit": "pnpm --filter @playground/sveltekit run sync",
    "dev:sveltekit": "pnpm --filter @playground/sveltekit run dev",
    "format:sveltekit": "pnpm --filter @playground/sveltekit run format",
    "lint:sveltekit": "pnpm --filter @playground/sveltekit run lint",
    "dev:vite": "pnpm --filter @playground/vite run dev",
    "format:vite": "pnpm --filter @playground/vite run format",
    "lint:vite": "pnpm --filter @playground/vite run lint"
  },
  "sideEffects": false,
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@hypernym/eslint-config": "^3.2.0",
    "@hypernym/prettier-config": "^3.1.0",
    "@hypernym/tsconfig": "^2.1.0",
    "@types/node": "^22.5.2",
    "eslint": "^9.9.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
