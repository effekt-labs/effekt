{
  "name": "effekt-workspace",
  "version": "0.0.0",
  "author": "Ivo Dolenc",
  "private": true,
  "type": "module",
  "scripts": {
    "format:root": "prettier --write . '!playgrounds' '!packages'",
    "format:effekt": "pnpm --filter effekt format",
    "lint:effekt": "pnpm --filter effekt lint",
    "test:effekt": "pnpm --filter effekt test",
    "test:utils:effekt": "pnpm --filter effekt test:utils",
    "test:easing:effekt": "pnpm --filter effekt test:easing",
    "check:effekt": "pnpm --filter effekt check",
    "update:effekt": "pnpm --filter effekt update",
    "build:effekt": "pnpm --filter effekt build",
    "publish:effekt": "pnpm --filter effekt publish:effekt",
    "sync:sveltekit": "pnpm --filter @playground/sveltekit sync",
    "dev:sveltekit": "pnpm --filter @playground/sveltekit dev",
    "format:sveltekit": "pnpm --filter @playground/sveltekit format",
    "lint:sveltekit": "pnpm --filter @playground/sveltekit lint",
    "dev:vite": "pnpm --filter @playground/vite dev",
    "format:vite": "pnpm --filter @playground/vite format",
    "lint:vite": "pnpm --filter @playground/vite lint"
  },
  "sideEffects": false,
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@hypernym/eslint-config": "^3.3.2",
    "@hypernym/prettier-config": "^3.2.0",
    "@hypernym/tsconfig": "^2.1.0",
    "@types/node": "^22.5.2",
    "eslint": "^9.9.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
